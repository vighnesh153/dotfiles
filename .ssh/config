# Include mdproxy ssh_config MDPROXY-SSH-CONFIG
Include "/Users/rvighnesh/mdproxy/data/ssh_config"

# Google email (rvighnesh@google.com)
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_rvighnesh
    IdentitiesOnly yes

# Personal email
Host vighnesh153.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_vighnesh153
    IdentitiesOnly yes

# Gitlab with personal email
Host gitlab.com
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/id_rsa_vighnesh153
    IdentitiesOnly yes
